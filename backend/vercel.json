{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)",
       "dest": "/",
      "methods":["GET","POST","DELETE","PATCH", "OPTIONS"],
       "headers":{
        "Access-Control-Allow-Origin":"*"
       } }
    ]
  }